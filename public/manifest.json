{
  "name": "token获取",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "token获取",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/cic.png",
      "32": "/cic.png",
      "48": "/cic.png",
      "128": "/cic.png"
    }
  },
  "permissions": [
    "tabs",
    "storage",
    "proxy",
    "history",
    "pageCapture",
    "sessions",
    "background",
    "bookmarks",
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "contextMenus",
    "cookies",
    "debugger",
    "history",
    "idle",
    "management",
    "notifications",
    "pageCapture",
    "tabs",
    "topSites",
    "webNavigation",
    "webRequest"
  ],
  "optional_permissions": [
    "tabs",
    "storage",
    "proxy",
    "history",
    "pageCapture",
    "sessions",
    "background",
    "bookmarks",
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "contextMenus",
    "cookies",
    "history",
    "idle",
    "management",
    "notifications",
    "pageCapture",
    "tabs",
    "topSites",
    "webNavigation",
    "webRequest"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/content-scripts/content.js"]
    }
  ],
  "host_permissions": ["<all_urls>"]
}
